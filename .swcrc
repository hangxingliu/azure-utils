{
  "sourceMaps": false,
  "module": {
    "type": "commonjs",
    "noInterop": true
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "privateMethod": true
    },
    "transform": {
      "decoratorMetadata": true
    },
    "target": "es2020",
    "preserveAllComments": true
  }
}
